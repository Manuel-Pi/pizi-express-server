{
  "name": "pizi-express-server",
  "version": "1.0.0",
  "description": "Simple RESTFull server",
  "main": "server.js",
  "scripts": {
    "start": "node ./server.js",
    "build": "tsc && node ./dist/server/services/test.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "startNodemon": "nodemon ./server.js --inspect",
    "clearPort": "kill -9 $(lsof -ti:8087)",
    "createCertificate": "cd ./server/certificates/ && openssl req -x509 -nodes -days 730 -newkey rsa:2048  -keyout cert.key -out cert.pem -config req.cnf -sha256",
    "getGmailAuth": "node -e 'require(\"./server/libs/gmailOauth\").getAuth()'"
  },
  "author": "Manuel Pi",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "googleapis": "^89.0.0",
    "helmet": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "memory-cache": "^0.2.0",
    "mongoose": "^6.0.12",
    "nodemailer": "^6.7.0",
    "nodemon": "^2.0.4",
    "socket.io": "^4.3.1",
    "socket.io-client": "^4.3.2",
    "tail": "^2.2.4",
    "webdav-server": "^2.6.2"
  },
  "engines": {
    "node": "16.x"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.4.3",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.63.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-middleware": "^5.2.1",
    "webpack-hot-middleware": "^2.25.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Manuel-Pi/pizi-express-server.git"
  }
}
